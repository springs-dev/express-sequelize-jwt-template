{
  "name": "express-sequelize-jwt-template",
  "version": "2.0.0",
  "description": "Express.js API Template for Springs",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "Express",
    "Sequelize",
    "Passport.js",
    "Springs"
  ],
  "author": "Springs",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:dev": "node --watch index.js | pino-pretty --ignore serviceContext",
    "db:migrate:up": "node sequelize-manager.js db:migrate | pino-pretty --ignore serviceContext && yarn db:seed:up",
    "db:migrate:undo": "node sequelize-manager.js db:migrate:undo | pino-pretty --ignore serviceContext",
    "db:seed:up": "node sequelize-manager.js db:seed:up | pino-pretty --ignore serviceContext",
    "db:seed:undo": "node sequelize-manager.js db:seed:undo | pino-pretty --ignore serviceContext",
    "db:erd": "node sequelize-erd.js",
    "generate:migration": "npx sequelize migration:generate --name",
    "generate:migration:add-columns": "npx yo ./_generators/add-columns-migration.js",
    "generate:seed": "npx yo ./_generators/generate-seed.js",
    "generate:model": "npx yo ./_generators/generate-model.js",
    "generate:scaffold": "npx yo ./_generators/scaffold.js",
    "stage": "yarn stage:delete; yarn stage:start",
    "stage:start": "NODE_ENV=production pm2 start ./index.js --name app-name-stage",
    "stage:restart": "NODE_ENV=production pm2 restart app-name-stage",
    "stage:delete": "pm2 delete app-name-stage",
    "prod": "yarn prod:delete; yarn prod:start",
    "prod:start": "NODE_ENV=production pm2 start ./index.js --name app-name-prod",
    "prod:restart": "NODE_ENV=production pm2 restart app-name-prod",
    "prod:delete": "pm2 delete app-name-prod",
    "prod:migrate": "NODE_ENV=production yarn db:migrate:up",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "node_modules/.bin/eslint --fix .",
    "prettier:fix": "node_modules/.bin/prettier --write .",
    "prepare": "node_modules/.bin/husky install",
    "depcheck": "npx depcheck --no-dev"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint:fix"
    ]
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "engines": {
    "node": ">=18.7.0 <=20.7.0"
  },
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^7.0.2",
    "form-data": "^4.0.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "mailgun.js": "^9.3.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pino-pretty": "^10.2.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.5.2",
    "sequelize-erd": "^1.3.1",
    "umzug": "^3.3.1",
    "yeoman-generator": "^5.9.0"
  }
}
