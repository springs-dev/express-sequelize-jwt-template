# This file is for unifying the coding style for different editors and IDEs: https://EditorConfig.org
# top-most EditorConfig file
# Can be overrigthed in subfolders
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
quote_type = double